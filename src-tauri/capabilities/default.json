{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:window:default",
    "core:window:allow-start-dragging",
    "core:default",
    "opener:default",
    "oauth:allow-start",
    "oauth:allow-cancel",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    "fs:allow-read-file",
    "fs:allow-read-dir",
    "fs:allow-copy-file",
    "fs:allow-remove",
    "fs:allow-rename",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "shell:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://open.feishu.cn/**"
        },
        {
          "url": "https://*.feishu.cn/**"
        },
        {
          "url": "https://*.larksuite.com/**"
        }
      ]
    },
    {
      "identifier": "opener:allow-open-path",
      "allow": [
        {
          "path": "/**"
        },
        {
          "path": "C:/**"
        },
        {
          "path": "D:/**"
        },
        {
          "path": "E:/**"
        },
        {
          "path": "F:/**"
        }
      ]
    },
    {
      "identifier": "fs:allow-write-file",
      "allow": [
        {
          "path": "/**"
        },
        {
          "path": "C:/**"
        },
        {
          "path": "D:/**"
        },
        {
          "path": "E:/**"
        },
        {
          "path": "F:/**"
        }
      ]
    },
    {
      "identifier": "fs:allow-mkdir",
      "allow": [
        {
          "path": "/**"
        },
        {
          "path": "C:/**"
        },
        {
          "path": "D:/**"
        },
        {
          "path": "E:/**"
        },
        {
          "path": "F:/**"
        }
      ]
    },
    {
      "identifier": "fs:allow-exists",
      "allow": [
        {
          "path": "/**"
        },
        {
          "path": "C:/**"
        },
        {
          "path": "D:/**"
        },
        {
          "path": "E:/**"
        },
        {
          "path": "F:/**"
        }
      ]
    }
  ]
}
